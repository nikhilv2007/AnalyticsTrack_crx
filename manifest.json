{
	"manifest_version":2,
	"name":"Analytics Tracker",
	"version":"0.0.0.2",
	"permissions": [ 
		"notifications", 
		"webRequest",
		"webRequestBlocking",
		"<all_urls>"
	],
	"description":"Track analytics calls on a website",
	"icons":{
		"16": "images/AT16.png",
        "48": "images/AT48.png",
    	"128": "images/AT128.png"
	},
	
	"minimum_chrome_version" : "22.0.0.0",
	"browser_action":{
		"default_icon":"images/AT48.png",
		"default_title":"Analytics Tracker",
		"default_popup":"popup/popup.html"
	},
	
	"background": {
		"scripts": [
			"background.js"
        ]
	}
}
